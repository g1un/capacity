mixin content()
	.container-fluid(class=attributes.class)
		.row
			-var i = 1
			while i <= 12
				div(class='col-md-1')
					div(style='background: #f2f2f2; height: 500px;')
				-i++
	.container-fluid(class=attributes.class)
		.row
			-var i = 1
			while i <= 12
				div(class='col-md-1')
					div(style='background: #f4f4f4; height: 1000px;')
				-i++